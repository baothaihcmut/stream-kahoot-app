model User {
    id                  String  @id
    firstName           String  @map("first_name")
    lastName            String  @map("last_name")
    email               String  @unique @map("email")
    picture             String @map("picture")
    currentRefreshToken String? @map("current_refresh_token")
    hostRooms               Room[] 
    participantRooms         RoomParticipant[] 
    @@map("users")
}